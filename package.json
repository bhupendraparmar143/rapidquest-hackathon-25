{
  "name": "audience-query-management",
  "version": "1.0.0",
  "description": "Unified system for managing audience queries across all channels",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "worker": "node server/workers/index.js",
    "client": "cd client && npm start",
    "client-install": "cd client && npm install",
    "dev-all": "concurrently \"npm run dev\" \"npm run worker\" \"npm run client\"",
    "install-all": "npm install && npm run client-install",
    "seed": "node server/scripts/seedData.js"
  },
  "keywords": [
    "query-management",
    "customer-support",
    "unified-inbox"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bull": "^4.16.5",
    "compromise": "^14.14.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mongoose": "^7.5.0",
    "natural": "^6.5.0",
    "nodemailer": "^6.10.1",
    "redis": "^4.6.10",
    "sentiment": "^5.0.2",
    "socket.io": "^4.8.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
